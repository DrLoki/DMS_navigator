{
  "name": "communitydmadminui5",
  "version": "0.2.0",
  "devDependencies": {
    "@sapui5/ts-types": "1.104.0",
    "@ui5/cli": "2.14.9",
    "@sap/ui5-builder-webide-extension": "1.1.8",
    "bestzip": "2.2.1",
    "rimraf": "3.0.2"
  },
  "scripts": {
    "build": "npm run clean && ui5 build --include-task=generateManifestBundle generateCachebusterInfo && npm run zip",
    "zip": "cd dist && npx bestzip ../communitydmadminui5-content.zip *",
    "clean": "npx rimraf communitydmadminui5-content.zip dist"
  },
  "ui5": {
    "dependencies": [
      "@sap/ui5-builder-webide-extension"
    ]
  }
}
